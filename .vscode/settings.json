{
    "metadata": {
        "file": ".vscode/settings.json",
        "description": "vscode folder settings",
        "version": "2023.02.22",
        "since": "2023.02.22",
        "author": {
            "name": "Amit M. Manthanwar",
            "email": "manthanwar@hotmail.com",
            "url": "https://manthanwar.github.io/"
        },
        "copyright": "(C) 2023 Amit M. Manthanwar",
        "license": "SEE LICENSE IN ./pages/license.html",
    },
    "[bash][sh]": {
        "editor.defaultFormatter": "foxundermoon.shell-format",
    },
    "[bibtex]": {
        "editor.defaultFormatter": "James-Yu.latex-workshop",
        // "editor.fontSize": 13
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools",
        "editor.wordBasedSuggestions": "off",
        "editor.suggest.insertMode": "replace",
        "editor.semanticHighlighting.enabled": true,
        "files.insertFinalNewline": true,
        "files.trimFinalNewlines": true
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        // "editor.defaultFormatter": "HookyQR.beautify",
        "editor.formatOnType": true,
        "editor.suggest.insertMode": "replace",
        "prettier.singleAttributePerLine": false,
        "editor.tabSize": 1,
        "editor.indentSize": "tabSize",
    },
    "[java]": {
        "editor.defaultFormatter": "redhat.java"
    },
    "[javascript]": {
        // "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.tabSize": 1,
        "editor.insertSpaces": true,
    },
    "[json][jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.tabSize": 4,
        "editor.indentSize": "tabSize",
        "editor.insertSpaces": true,
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[latex][tex]": {
        "editor.defaultFormatter": "James-Yu.latex-workshop",
        // "editor.lineNumbers": "on",
        "editor.tabSize": 1,
        "editor.indentSize": "tabSize",
        "editor.insertSpaces": true,
        "editor.detectIndentation": false,
        "editor.autoIndent": "advanced",
        // "editor.minimap.enabled": false,
        // "editor.quickSuggestions": {
        //     "comments": "off",
        //     "other": "off",
        //     "strings": "off"
        // },
        "editor.formatOnSave": true,
        "editor.formatOnPaste": true,
        "editor.formatOnType": false,
    },
    "[Log]": {
        // "editor.fontSize": 15
    },
    "[makefile]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.detectIndentation": false
    },
    "[markdown]": {
        // "editor.fontFamily": "Source Code Pro, monospace",
        // "editor.fontSize": 14,
        // "editor.lineHeight": 22,
        // "editor.lineNumbers": "off",
        // "editor.minimap.enabled": false,
        // "editor.quickSuggestions": {
        //     "comments": "off",
        //     "other": "off",
        //     "strings": "off"
        // },
        // "editor.wordWrap": "wordWrapColumn",
        // "editor.wordWrapColumn": 80,
        // "files.trimTrailingWhitespace": false,

    },
    "markdownlint.config": { "extends": ".vscode/markdownlint.json" },
    "markdown.preview.fontSize": 16,
    "markdown.preview.lineHeight": 1.5,
    "markdown.preview.scrollPreviewWithEditor": true,
    "markdown.preview.scrollEditorWithPreview": true,
    // "markdown.styles": [ ".vscode/md-style.css" ],
    "[plaintext]": {
        // "editor.fontSize": 14,
        "editor.lineHeight": 20,
        "files.insertFinalNewline": false,
        "editor.wordWrapColumn": 80,
    },
    "[plaintext][latex][tex]": {
        "editor.wordWrap": "wordWrapColumn",
        "editor.wrappingIndent": "same",
        "editor.wordWrapColumn": 80,
        "editor.formatOnPaste": true,
        "editor.formatOnType": false,
        "editor.formatOnSave": true
    },
    "python.formatting.autopep8Path": ".venv/bin/autopep8",
    // "autopep8.path": [
    //     ".vscode/autopep8"
    // ],
    // "autopep8.args": [
    //     "--indent-size=2",
    //     "--ignore E121"
    // ],
    "[python]": {
        "editor.defaultFormatter": "ms-python.autopep8",
        // "editor.defaultFormatter": "ms-python.black-formatter",
        // "editor.fontSize": 14,
        "editor.tabSize": 4,
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.formatOnPaste": true,
    },
    "[shellscript]": {
        "editor.minimap.enabled": false,
        "editor.quickSuggestions": {
            "other": "off",
            "comments": "off",
            "strings": "off"
        },
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "[yaml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.insertSpaces": true,
        "diffEditor.ignoreTrimWhitespace": false,
        "prettier.useTabs": false,
    },
    "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 4, ColumnLimit: 0}",
    "cmake.configureOnOpen": false,
    "cSpell.language": "en",
    // "cSpell.language": "en-GB",
    "cSpell.words": [
        "cyberphysical",
        "titlepage"
    ],
    "diffEditor.renderSideBySide": true,
    "debug.console.fontSize": 14,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit",
        "source.fixAll.tslint": "explicit",
        "source.fixAll.markdownlint": "explicit"
    },
    "editor.accessibilitySupport": "off",
    "editor.renderLineHighlight": "gutter",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.fontFamily": "Fira Code, Source Code Pro, Consolas, monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "editor.fontWeight": "300",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "file",
    "editor.formatOnType": true,
    "editor.guides.bracketPairs": true,
    "editor.indentSize": 4,
    "editor.insertSpaces": true,
    "editor.lineHeight": 20,
    "editor.minimap.enabled": true,
    "editor.minimap.maxColumn": 40,
    "editor.minimap.renderCharacters": false,
    "editor.mouseWheelZoom": true,
    "editor.multiCursorModifier": "alt",
    "editor.quickSuggestions": {
        "comments": "on",
        "strings": "on",
        "other": "on"
    },
    "editor.renderWhitespace": "all",
    "editor.rulers": [
        80,
        100,
        120
    ],
    // "editor.tabSize": 1,
    "editor.tabCompletion": "on",

    "editor.tokenColorCustomizations": {
        "comments": "#baa",
