    "latex-workshop.linting.chktex.enabled": true,
    "latex-workshop.linting.lacheck.enabled": true,
    "latex-workshop.linting.chktex.exec.path": ".vscode/chktexrc",
    "latex-workshop.linting.lacheck.exec.path": ".vscode/lacheck",
    "latex-workshop.linting.run": "onType",
    "latex-workshop.check.duplicatedLabels.enabled": false,
    "latex-workshop.formatting.latexindent.args": [
        "-cruft=%DIR%/build/",
        "-logfile=latexindent.log",
        "%TMPFILE%",
        "--local=%WORKSPACE_FOLDER%/.vscode/latexindent.yaml"
