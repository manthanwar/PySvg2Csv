    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80,
    "eslint.validate": [
        "javascript"
    ],
    "explorer.sortOrder": "type",
    "files.associations": {
        ".code-snippets": "json",
        "*.awk": "sh",
        "*.clang-format": "yaml",
        "*.cpp": "cpp",
        "*.hbs": "html",
        "*.jsx": "javascript",
        "*.md.html": "markdown",
        "*.bib": "bibtex",
        "*.cls": "latex-class",
        "*.fd": "latex",
        "*.sty": "latex",
        "*.tex": "latex",
        "*.svg": "html",
        "*.thor": "ruby",
        "*.xml": "html",
        "Jenkinsfile*": "groovy",
        "numeric": "cpp",
    },
    // "files.autoSave": "onFocusChange",
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 1000,
    "files.encoding": "utf8",
    "files.eol": "\n",
    "files.exclude": {
        ".git": true,
        ".gitignore": true,
        // ".vscode": true,
        ".eslintrc.json": true,
        "package-lock.json": true,
        "node_modules": true,
        "**/.git": true,
        "**/*.aux": true,
        "**/*.bbl": true,
        "**/*.bcf": true,
        "**/*.blg": true,
        "**/*.fdb_latexmk": true,
        "**/*.fls": true,
        "**/*.glg": true,
        "**/*.glo": true,
        "**/*.gls": true,
        "**/*.glsdefs": true,
        "**/*.idx": true,
        "**/*.ilg": true,
        "**/*.ind": true,
        "**/*.lof": true,
        "**/*.log": true,
        "**/*.lot": true,
        "**/*.mw": true,
        "**/*.nlo": true,
        "**/*.nls": true,
        "**/*.out": true,
        "**/*.run.xml": true,
        "**/*.synctex.gz": true,
        "**/*.thm": true,
        "**/*.toc": true,
        "**/*.xdy": true
    },
    "files.trimTrailingWhitespace": true,
    "git.promptToSaveFilesBeforeCommit": "always",
    "html.format.enable": true,
    "html.format.indentHandlebars": true,
    "html.format.indentInnerHtml": true,
    "html.format.wrapAttributes": "force-aligned",
    "html.format.wrapLineLength": 0,
    "json.format.keepLines": true,
