                //-------------------------------------------------------------
                //#region Python
                //-------------------------------------------------------------
                {
                    "name": "source.python.dec",
                    "scope": [
                        "constant.numeric.dec.python",
                        "constant.numeric.float.python"
                    ],
                    "settings": {
                        "foreground": "#e5f437"
                    }
                },
                {
                    "name": "variable",
                    "scope": [
